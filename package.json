{
  "name": "playbooks-interface",
  "version": "1.0.0",
  "description": "A Vite / TypeScript npm package template",
  "license": "ISC",
  "type": "module",
  "main": "dist/index.js",
  "engines": {
    "node": ">=16"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./accordions": {
      "import": "./dist/accordions.js",
      "types": "./dist/accordions.d.ts"
    },
    "./alerts": {
      "import": "./dist/alerts.js",
      "types": "./dist/alerts.d.ts"
    },
    "./avatars": {
      "import": "./dist/avatars.js",
      "types": "./dist/avatars.d.ts"
    },
    "./badges": {
      "import": "./dist/badges.js",
      "types": "./dist/badges.d.ts"
    },
    "./banners": {
      "import": "./dist/banners.js",
      "types": "./dist/banners.d.ts"
    },
    "./breadrumbs": {
      "import": "./dist/breadrumbs.js",
      "types": "./dist/breadrumbs.d.ts"
    },
    "./button-groups": {
      "import": "./dist/button-groups.js",
      "types": "./dist/button-groups.d.ts"
    },
    "./buttons": {
      "import": "./dist/buttons.js",
      "types": "./dist/buttons.d.ts"
    },
    "./cards": {
      "import": "./dist/cards.js",
      "types": "./dist/cards.d.ts"
    },
    "./drops": {
      "import": "./dist/drops.js",
      "types": "./dist/drops.d.ts"
    },
    "./feedbacks": {
      "import": "./dist/feedbacks.js",
      "types": "./dist/feedbacks.d.ts"
    },
    "./fonts": {
      "import": "./dist/fonts.js",
      "types": "./dist/fonts.d.ts"
    },
    "./footers": {
      "import": "./dist/footers.js",
      "types": "./dist/footers.d.ts"
    },
    "./forms": {
      "import": "./dist/forms.js",
      "types": "./dist/forms.d.ts"
    },
    "./grid": {
      "import": "./dist/grid.js",
      "types": "./dist/grid.d.ts"
    },
    "./head": {
      "import": "./dist/head.js",
      "types": "./dist/head.d.ts"
    },
    "./headers": {
      "import": "./dist/headers.js",
      "types": "./dist/headers.d.ts"
    },
    "./heros": {
      "import": "./dist/heros.js",
      "types": "./dist/heros.d.ts"
    },
    "./html": {
      "import": "./dist/html.js",
      "types": "./dist/html.d.ts"
    },
    "./icons": {
      "import": "./dist/icons.js",
      "types": "./dist/icons.d.ts"
    },
    "./dist/styles.css": "./dist/style.css"
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "start": "vite build --mode development --watch --minify false",
    "build": "vite build --mode production --minify false",
    "build:ts": "dts-bundle-generator --config dts.config.ts",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier src",
    "format:fix": "prettier --write src",
    "clean": "npm run lint:fix && npm run format:fix",
    "pregit": "git add .",
    "git": "git push --tags",
    "test": "jest",
    "prepare": "husky install",
    "links": "npm link ../playbook-web/node_modules/react",
    "unlinks": "npm link ../playbook-web/node_modules/react"
  },
  "peerDependencies": {
    "next": "14.2.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-brands-svg-icons": "^6.4.2",
    "@fortawesome/pro-duotone-svg-icons": "^6.4.0",
    "@fortawesome/pro-regular-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@react-spring/web": "^9.7.3",
    "dts-bundle-generator": "^9.5.1",
    "react-currency-input-field": "^3.8.0",
    "react-input-mask": "^2.0.4",
    "react-phone-number-input": "^3.4.3",
    "react-popper": "^2.3.0",
    "react-range": "^1.8.14",
    "react-transition-group": "^4.4.5",
    "svg-loaders-react": "^2.2.1"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.7",
    "@types/jest": "^29.5.1",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "@vitejs/plugin-react": "^3.0.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "postcss": "^8.4.39",
    "prettier": "^2.8.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "tailwindcss": "^3.4.4",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4",
    "vite": "^4.0.0",
    "vite-plugin-dts": "^3.9.1"
  }
}
